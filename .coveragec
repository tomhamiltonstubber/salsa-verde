[run]
source = SalsaVerde
branch = True
concurrency = multiprocessing
omit =
    */management/commands/*
    *standard_test.py
    *wsgi.py
    *test_settings.py
    */migrations/0*.py
parallel = True

[report]
exclude_lines =
    pragma: no cover

    def __repr__
    def __unicode__
    def __str__

    raise AssertionError
    raise NotImplementedError
    print
