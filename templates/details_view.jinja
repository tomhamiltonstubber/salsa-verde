{% extends 'auth.jinja' %}

{% block content %}
  <div class="row">
    <div id="display-items">
      {% for field_name, v in display_items %}
        <div>
          <label class="text-muted">{{ field_name }}:</label> <span class="display-item">{{ v }}</span>
        </div>
      {% endfor %}
    </div>
  </div>
{% endblock %}
